<p id="notice"><%= notice %></p>

<h1>Listing Wishlists</h1>

<table>
  <thead>
    <tr>
      <th>Room</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @wishlists.each do |wishlist| %>
      <tr>
        <td><%= wishlist.room.name %></td>
        <td><%= wishlist.user.email %></td>
        
        <td><%= link_to 'Remove from wishlist', wishlist, method: :delete, 
        data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

